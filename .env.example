# Backend Environment Variables
# Copy this file to .env and fill in the values

# Google OAuth (for backend token verification)
GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com

# JWT Secret (generate a secure random string for production)
JWT_SECRET=your-secure-jwt-secret-change-in-production

# Database URL (optional, defaults to SQLite)
DATABASE_URL=sqlite:///./data/dga_detection.db

# Model Paths (optional, defaults to models/ directory)
RF_MODEL_PATH=models/random_forest.joblib
LSTM_MODEL_PATH=models/lstm
